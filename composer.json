{
    "name": "wieloco/ps-matcha",
    "description": "Custom matchers for PhpSpec",
    "keywords": [
        "phpspec",
        "matchers",
        "tdd",
        "uuid"
    ],
    "type": "phpspec-extension",
    "require": {
        "phpspec/phpspec": "^7.0",
        "php": ">=7.3",
        "ramsey/uuid": "^3.7|^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Krzysztof Wielog√≥rski",
            "email": "contact@wielo.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "WieloCo\\Matcha\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpspec run",
        "cs": "vendor/bin/php-cs-fixer fix"
    }
}
